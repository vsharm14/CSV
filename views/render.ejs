<style>
  #habitTable {
  font-family: Arial, Helvetica, sans-serif;
  border-collapse: collapse;
  width: 100%;
}



#habitTable{
    margin-top: 10px;
}
#habitTable td, #habitTable th {
  border: 1px solid #ddd;
  padding: 8px;
  min-width : 100px;
}
ul{
  list-style: none;
}

li{
  padding : 10px;
  height : 40px;

}
a{
    text-decoration: none;
    font-size : 40px;
    color :aquamarin; 
    margin-bottom : 40px;   
}
a:hover{
    font-size : 45px;
}

#habitTable tr:nth-child(even){background-color: #f2f2f2;}

#habitTable tr:hover {background-color: #ddd;}

#habitTable th,td{
    min-width: 4vw;
}

#habitTable th {
  padding-top: 12px;
  padding-bottom: 12px;
  text-align: left;
  background-color: #04AA6D;
  color: white;
}
  .container{
    background-color: beige;
  }
</style>

<Render>
    <a href="/">Analyze Other FIle</a>
    <table id="habitTable">
    <tr>
        <% data.forEach((row,index) => { %>
            <tr>
                <% row.forEach((dt) => { %>
                    <th><%= dt %></th>
                <% }) %>
            </tr>
        <% }) %>
    </tr>
     </table>
</Render>